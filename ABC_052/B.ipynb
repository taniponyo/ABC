    "N = int(input())\n",
    "S = input()\n",
    "\n",
    "x = 0\n",
    "ans = 0\n",
    "\n",
    "for i in range(N):\n",
    "    if S[i] == 'I':\n",
    "        x += 1\n",
    "    else:\n",
    "        x -= 1\n",
    "        \n",
    "    ans = max(ans,x)\n",
    "    \n",
    "print(ans)"
