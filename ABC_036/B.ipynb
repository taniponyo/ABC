    "N = int(input())\n",
    "s = [input() for _ in range(N)]\n",
    "\n",
    "tmp = [[] for i in range(N)]\n",
    "\n",
    "for i in range(N):\n",
    "    for j in range(N):\n",
    "        tmp[i].append(s[N-1-j][i])\n",
    "\n",
    "\n",
    "for i in range(N):\n",
    "    print(''.join(tmp[i]))"
   ]
  }
