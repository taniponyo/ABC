    "N = int(input())\n",
    "S = []\n",
    "P = []\n",
    "for i in range(N):\n",
    "    s,p = map(str,input().split())\n",
    "    S.append(s)\n",
    "    P.append(int(p))\n",
    "\n",
    "import sys\n",
    "\n",
    "tmp_sum = sum(P)//2\n",
    "for i in range(N):\n",
    "    if tmp_sum < P[i]:\n",
    "        print(S[i])\n",
    "        sys.exit()\n",
    "print('atcoder')"
   ]
