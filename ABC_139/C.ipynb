    "N = int(input())\n",
    "H = list(map(int,input().split())) + [10**9+7]\n",
    "\n",
    "count = 0\n",
    "ans = 0\n",
    "\n",
    "for i in range(N):\n",
    "    if H[i] >= H[i+1]:\n",
    "        count += 1\n",
    "    else:\n",
    "        ans = max(ans,count)\n",
    "        count = 0\n",
    "\n",
    "print(ans)"
