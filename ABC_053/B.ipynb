    "        start = i\n",
    "        break\n",
    "        \n",
    "for i in range(len(s)):\n",
    "    if s[len(s)-i-1] == 'Z':\n",
    "        end = i\n",
    "        break\n",
    "    \n",
    "print(len(s) - start - end)"
   ]
