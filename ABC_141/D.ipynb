   "source": [
    "N,M = map(int,input().split())\n",
    "A = list(map(int,input().split()))\n",
    "\n",
    "import heapq\n",
    "for i in range(N):\n",
    "    A[i] = -A[i]\n",
    "    \n",
    "heapq.heapify(A)\n",
    "\n",
    "while M > 0:\n",
    "    tmp = -heapq.heappop(A)\n",
    "    tmp = tmp//2\n",
    "    M -= 1\n",
    "    heapq.heappush(A,-tmp)\n",
    "    \n",
    "print(-sum(A))"
   ]
  }
 ],
