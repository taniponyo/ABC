    "N = int(input())\n",
    "s = [0]*N\n",
    "for i in range(N):\n",
    "    s[i] = int(input())\n",
    "\n",
    "if sum(s) % 10 != 0:\n",
    "    print(sum(s))\n",
    "else:\n",
    "    ans = 0\n",
    "    tmp = sum(s)\n",
    "    for i in range(N):\n",
    "        if (tmp - s[i]) % 10 != 0:\n",
    "            ans = max(ans,tmp - s[i])\n",
    "    print(ans)"
   ]
  }
