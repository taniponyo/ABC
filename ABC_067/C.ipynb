   "source": [
    "ans = 10**18+1\n",
    "#ループの中にsum入れるとロス\n",
    "sum_a = sum(a)\n",
    "\n",
    "for i in range(N-1):\n",
    "    ans = min(ans,abs(sum_a-2*a_sum[i]))\n",
    "    \n",
    "print(ans)"
   ]
