    " \n",
    "deg = [0] * (N+1)\n",
    " \n",
    "for _ in range(M):\n",
    "  a,b = map(int,input().split())\n",
    "  deg[a] += 1\n",
    "  deg[b] += 1\n",
    " \n",
    "print(*deg[1:],sep='\\n')"
