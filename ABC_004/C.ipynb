    "N = int(input())\n",
    "\n",
    "loop = N//5\n",
    "start = loop%6 + 1\n",
    "\n",
    "base = []\n",
    "\n",
    "for i in range(6):\n",
    "    base.append(str(start))\n",
    "    start += 1\n",
    "    if start > 6:\n",
    "        start = 1\n",
    "    \n",
    "res = N%5\n",
    "\n",
    "for i in range(res):\n",
    "    base[i],base[i+1] = base[i+1],base[i]\n",
    "\n",
    "print(''.join(base))"
   ]
  }
