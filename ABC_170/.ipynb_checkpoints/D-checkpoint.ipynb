    "N = int(input())\n",
    "A = list(map(int,input().split()))\n",
    "A.sort()\n",
    "MAX = A[-1]+1\n",
    "num_tmp = [0]*MAX\n",
    "\n",
    "for a in A:\n",
    "    num_tmp[a] += 1\n",
    "    if num_tmp[a] == 1:\n",
    "        for i in range(2*a,MAX,a):\n",
    "            num_tmp[i] = 2\n",
    "\n",
    "print(num_tmp.count(1))"
   ]
