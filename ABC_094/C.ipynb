   "source": [
    "N = int(input())\n",
    "X = list(map(int,input().split()))\n",
    " \n",
    "X_sort = sorted(X)\n",
    " \n",
    "mid_r = X_sort[N//2]\n",
    "mid_l = X_sort[N//2-1]\n",
    " \n",
    "for i in range(N):\n",
    "    if X[i] <= mid_l:\n",
    "        print(mid_r)\n",
    "    else:\n",
    "        print(mid_l)"
   ]
  }
