   "source": [
    "N,A,B = map(int,input().split())\n",
    "\n",
    "ans = 0\n",
    "\n",
    "for i in range(1,N+1):\n",
    "    tmp = str(i)\n",
    "    wa = 0\n",
    "    for j in tmp:\n",
    "        wa += int(j)\n",
    "    if A <= wa and wa <= B:\n",
    "        ans += i\n",
    "        \n",
    "print(ans)"
   ]
  }
