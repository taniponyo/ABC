   "source": [
    "N = int(input())\n",
    "A = list(map(int,input().split()))\n",
    "\n",
    "SUM = sum(A)\n",
    "tmp = 0\n",
    "for i in range(1,N,2):\n",
    "    tmp += A[i]\n",
    "ans = [0]*N\n",
    "ans[0] = SUM - 2*tmp\n",
    "for i in range(1,N):\n",
    "    ans[i] = 2*A[i-1] - ans[i-1]\n",
    "print(*ans)"
   ]
  }
