    "N = int(input())\n",
    "AB = [list(map(int,input().split())) for i in range(N)]\n",
    "\n",
    "sort_AB = sorted(AB, key= lambda x: x[1])\n",
    "ans = 'Yes'\n",
    "tmp = 0\n",
    "\n",
    "for i in range(N):\n",
    "    tmp += sort_AB[i][0]\n",
    "    if tmp > sort_AB[i][1]:\n",
    "        ans = 'No'\n",
    "        break\n",
    "        \n",
    "print(ans)"
   ]
