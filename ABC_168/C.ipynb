   "source": [
    "A,B,H,M = map(int,input().split())\n",
    "\n",
    "import math\n",
    "\n",
    "H_tmp = 2*math.pi*(H/12) + (2*math.pi/12)*(M/60)\n",
    "M_tmp = 2*math.pi*(M/60)\n",
    "\n",
    "kakudo = min(abs(H_tmp-M_tmp),2*math.pi - abs(H_tmp-M_tmp))\n",
    "\n",
    "ans = (A**2 + B**2 - 2*A*B*math.cos(kakudo))**0.5\n",
    "\n",
    "print(ans)"
   ]
  }
