    "N = int(input())\n",
    "T = [0] + [int(x) for x in input().split()]\n",
    "M = int(input())\n",
    "s = sum(T)\n",
    " \n",
    "answer = []\n",
    "for _ in range(M):\n",
    "  a,b = map(int,input().split())\n",
    "  answer.append(s+b-T[a])\n",
    " \n",
    "print(*answer,sep='\\n')"
   ]
