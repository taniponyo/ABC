   "source": [
    "n = int(input())\n",
    " \n",
    "ans = []\n",
    "while n != 0:\n",
    "    if n % 2 != 0:\n",
    "        n -= 1\n",
    "        ans.append(\"1\")\n",
    "    else:\n",
    "        ans.append(\"0\")\n",
    "    n //= -2\n",
    " \n",
    "if len(ans) == 0:\n",
    "    ans.append(\"0\")\n",
    " \n",
    "print(\"\".join(reversed(ans)))"
   ]
  }
