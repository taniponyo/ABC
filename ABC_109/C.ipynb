    "N,X = list(map(int,input().split()))\n",
    "x = list(map(int,input().split()))\n",
    " \n",
    "number = []\n",
    " \n",
    "for i in range(N):\n",
    "    number.append(abs(x[i]-X))\n",
    " \n",
    "import fractions \n",
    " \n",
    "ans = number[0]\n",
    "for i in range(1, N):\n",
    "    ans = fractions.gcd(ans, number[i])\n",
    "    \n",
    "print(ans)"
   ]
