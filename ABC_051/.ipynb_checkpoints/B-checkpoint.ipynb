    "K, S = list(map(int,input().split()))\n",
    " \n",
    "count = 0\n",
    " \n",
    "for x in range(K+1):\n",
    "    for y in range(K+1):\n",
    "        if S-x-y >= 0 and K >= S-x-y:\n",
    "            count += 1\n",
    "            \n",
    "print(count)"
   ]
